{
    "extends": "../tsconfig.json",
    "include": [
        "*/*.ts",
        "integration/*.ts",
        "support/*.ts",
        "../node_modules/cypress"
    ],
    "compilerOptions":{
        "strict": true,
        "baseUrl": "../node_modules",
        "target": "es5",
        "lib": ["es5", "dom"],
        "types": ["cypress"]
    },
    "types": [
        // load Cypress global type
        "cypress"
    ],
    "exclude": [
    	"../node_modules"
    ]
}