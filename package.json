{
    "name": "cyts",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "--- @name Semantic": "--- @desc Semantic commit and release",
        "commit": "git-cz",
        "--- @name Test": "--- @desc test suite",
        "test": "yarn run test:unit && yarn run test:bdd",
        "test:unit": "jest",
        "test:bdd": "cypress run --spec **/*.feature",
        "test:e2e": "start-server-and-test start:local http://localhost:8081 test:bdd",
        "test:e2e:record": "cypress run --record",
        "test:cy:open": "cypress open",
        "--- @name WebPack": "--- @desc Run, Build and Deploy",
        "start:local": "NODE_ENV=local webpack-dev-server --open --hot --config webpack.dev.js",
        "start:dev": "NODE_ENV=development webpack-dev-server --open --hot --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "--- @name Storybook": "--- @desc Run, build and deploy storybook",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -c .storybook -o docs/storybook",
        "deploy-storybook": "yarn run build-storybook && surge ./docs/storybook/ sca-story.surge.sh",
        "--- @name AWS:ECR": "--- @desc AWS ECR Cloudformation scripts",
        "aws:ecr:create": "cd ./devops/aws/ecr/ && sh create-ecr-stack.sh",
        "aws:ecr:update": "cd ./devops/aws/ecr/ && sh update-ecr-stack.sh",
        "aws:ecr:delete": "cd ./devops/aws/ecr/ && sh delete-ecr-stack.sh",
        "aws:ecr:validate": "cd ./devops/aws/ecr/ && sh validate-ecr-stack.sh",
        "--- @name AWS:LoadBalancer": "--- @desc AWS LoadBalancer Cloudformation scripts",
        "aws:loadbalancer:create": "cd ./devops/aws/loadbalancer/ && sh create-loadbalancer-stack.sh",
        "aws:loadbalancer:update": "cd ./devops/aws/loadbalancer/ && sh update-loadbalancer-stack.sh",
        "aws:loadbalancer:delete": "cd ./devops/aws/loadbalancer/ && sh delete-loadbalancer-stack.sh",
        "aws:loadbalancer:validate": "cd ./devops/aws/loadbalancer/ && sh validate-loadbalancer-stack.sh",
        "--- @name AWS:ECS": "--- @desc AWS ECS Cloudformation scripts",
        "aws:ecs:create": "cd ./devops/aws/ecs/ && sh create-ecs-stack.sh",
        "aws:ecs:update": "cd ./devops/aws/ecs/ && sh update-ecs-stack.sh",
        "aws:ecs:delete": "cd ./devops/aws/ecs/ && sh delete-ecs-stack.sh",
        "aws:ecs:validate": "cd ./devops/aws/ecs/ && sh validate-ecs-stack.sh",
        "--- @name AWS": "--- @desc AWS Cloudformation scripts",
        "aws:create": "yarn run aws:ecr:create && yarn run aws:ecs:create ",
        "aws:update": "yarn run aws:ecr:update && yarn run aws:ecs:update ",
        "aws:delete": "yarn run aws:ecr:delete && yarn run aws:ecs:delete",
        "aws:validate": "yarn run aws:ecr:validate && yarn run aws:ecs:validate"
    },
    "dependencies": {
        "@material-ui/core": "^4.5.1",
        "@types/axios": "^0.14.0",
        "@types/enzyme": "^3.10.3",
        "@types/react": "^16.9.9",
        "@types/react-dom": "^16.9.2",
        "@types/yup": "^0.26.24",
        "axios": "^0.19.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "formik": "^1.5.8",
        "formik-material-ui": "^0.0.22",
        "lodash": "^4.17.21",
        "react": "^16.10.2",
        "react-dom": "^16.10.2",
        "react-router-dom": "^5.1.2",
        "react-test-renderer": "^16.10.2",
        "winston": "^3.2.1",
        "yup": "^0.27.0"
    },
    "devDependencies": {
        "@bahmutov/add-typescript-to-cypress": "^2.1.2",
        "@cypress/webpack-preprocessor": "^4.1.0",
        "@types/chai": "^4.1.7",
        "@types/cypress": "^1.1.3",
        "@types/jest": "^24.0.19",
        "@types/lodash": "^4.14.144",
        "awesome-typescript-loader": "^5.2.1",
        "babel-polyfill": "^6.26.0",
        "copy-webpack-plugin": "^5.0.4",
        "cypress": "^3.4.1",
        "cypress-cucumber-preprocessor": "^1.16.2",
        "cz-conventional-changelog": "3.0.2",
        "enzyme-to-json": "^3.4.3",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "jest-cucumber": "^2.0.11",
        "sinon": "^7.5.0",
        "source-map-loader": "^0.2.4",
        "start-server-and-test": "^1.10.6",
        "ts-jest": "24.1.0",
        "ts-loader": "^6.2.0",
        "typescript": "^3.6.4",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2",
        "webpack-merge": "^4.2.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": true
    },
    "jest": {
        "testEnvironment": "node"
    }
}
